{
    "@context": "https://schema.org/",
    "@type": "PrimitiveAction",
    "name": "ASSERT",
    "description": "Asserts a proposition as true",
    "input": {
      "@type": "Thing",
      "description": "The proposition to be asserted"
    },
    "output": {
      "@type": "Thing",
      "description": "The resulting change to the knowledge base"
    },
    "example": {
      "input": {
        "@type": "Proposition",
        "description": "John is a student"
      },
      "output": {
        "@type": "KnowledgeBase",
        "description": "John is now recognized as a student"
      }
    }
  }
  