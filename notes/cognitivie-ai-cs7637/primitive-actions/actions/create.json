{
    "@context": "https://schema.org/",
    "@type": "PrimitiveAction",
    "name": "CREATE",
    "description": "Creates a new concept or object",
    "input": {
      "@type": "Thing",
      "description": "The parameters for creating the new concept or object"
    },
    "output": {
      "@type": "Thing",
      "description": "The newly created concept or object"
    },
    "example": {
      "input": {
        "@type": "Parameters",
        "description": {
          "name": "John",
          "age": 35,
          "occupation": "teacher"
        }
      },
      "output": {
        "@type": "Concept",
        "description": "John",
        "attributes": [
          {
            "@type": "Attribute",
            "description": "John",
            "name": "age",
            "value": 35
          },
          {
            "@type": "Attribute",
            "description": "John",
            "name": "occupation",
            "value": "teacher"
          }
        ]
      }
    }
  }
  