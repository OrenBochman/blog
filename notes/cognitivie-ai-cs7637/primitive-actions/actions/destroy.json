{
    "@context": "https://schema.org/",
    "@type": "PrimitiveAction",
    "name": "DESTROY",
    "description": "Removes a concept or object from the knowledge base",
    "input": {
      "@type": "Thing",
      "description": "The concept or object to be removed"
    },
    "output": {
      "@type": "Thing",
      "description": "None"
    },
    "example": {
      "input": {
        "@type": "Concept",
        "description": "John",
        "attributes": [
          {
            "@type": "Attribute",
            "description": "John",
            "name": "age",
            "value": 35
          },
          {
            "@type": "Attribute",
            "description": "John",
            "name": "occupation",
            "value": "teacher"
          }
        ]
      },
      "output": {
        "@type": "Thing",
        "description": "None"
      }
    }
  }
  