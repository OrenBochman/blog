{
    "@context": "https://schema.org/",
    "@type": "PrimitiveAction",
    "name": "GROUP",
    "description": "Groups concepts together based on a shared attribute or relationship",
    "input": {
      "@type": "Thing",
      "description": "The concepts to be grouped"
    },
    "output": {
      "@type": "Thing",
      "description": "The resulting group of concepts"
    },
    "example": {
      "input": {
        "@type": "List",
        "description": [
          "cat",
          "dog",
          "hamster",
          "parrot"
        ]
      },
      "output": {
        "@type": "Group",
        "description": "Animals",
        "members": [
          "cat",
          "dog",
          "hamster",
          "parrot"
        ]
      }
    }
  }
  