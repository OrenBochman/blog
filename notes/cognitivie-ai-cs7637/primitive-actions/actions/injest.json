{
    "@context": "https://schema.org/",
    "@type": "PrimitiveAction",
    "name": "INGEST",
    "description": "Introduces an object into an agent's body",
    "input": {
      "@type": "Thing",
      "description": "The object to be ingested"
    },
    "output": {
      "@type": "Thing",
      "description": "The ingested object"
    },
    "example": {
      "input": {
        "@type": "Concept",
        "description": "Frog",
        "attributes": [
          {
            "@type": "Attribute",
            "description": "Frog",
            "name": "location",
            "value": "Pond"
          }
        ]
      },
      "output": {
        "@type": "Concept",
        "description": "Frog",
        "attributes": [
          {
            "@type": "Attribute",
            "description": "Frog",
            "name": "location",
            "value": "Asok's Stomach"
          }
        ]
      }
    }
  }
  