{
    "@context": "https://schema.org/",
    "@type": "PrimitiveAction",
    "name": "MOVE-OBJECT",
    "description": "Changes the location of an object",
    "input": {
      "@type": "Thing",
      "description": "The object to be moved"
    },
    "output": {
      "@type": "Thing",
      "description": "The moved object"
    },
    "example": {
      "input": {
        "@type": "Concept",
        "description": "Book",
        "attributes": [
          {
            "@type": "Attribute",
            "description": "Book",
            "name": "location",
            "value": "Living Room"
          }
        ]
      },
      "output": {
        "@type": "Concept",
        "description": "Book",
        "attributes": [
          {
            "@type": "Attribute",
            "description": "Book",
            "name": "location",
            "value": "Bedroom"
          }
        ]
      }
    }
  }
  