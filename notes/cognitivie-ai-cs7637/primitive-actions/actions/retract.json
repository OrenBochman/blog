{
  "@context": "https://schema.org/",
  "@type": "PrimitiveAction",
  "name": "RETRACT",
  "description": "Retracts a previously asserted proposition",
  "input": {
    "@type": "Thing",
    "description": "The proposition to be retracted"
  },
  "output": {
    "@type": "Thing",
    "description": "The resulting change to the knowledge base"
  },
  "example": {
    "input": {
      "@type": "Proposition",
      "description": "John is a student"
    },
    "output": {
      "@type": "KnowledgeBase",
      "description": "John is no longer recognized as a student"
    }
  }
}
