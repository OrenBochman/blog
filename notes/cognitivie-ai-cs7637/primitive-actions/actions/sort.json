{
    "@context": "https://schema.org/",
    "@type": "PrimitiveAction",
    "name": "SORT",
    "description": "Sorts concepts into a specified order based on a specified attribute or relationship",
    "input": {
      "@type": "Thing",
      "description": "The concepts to be sorted"
    },
    "output": {
      "@type": "Thing",
      "description": "The resulting sorted list of concepts"
    },
    "example": {
      "input": {
        "@type": "List",
        "description": [
          "John",
          "Sue",
          "Bob",
          "Alice"
        ]
      },
      "output": {
        "@type": "List",
        "description": [
          "Alice",
          "Bob",
          "John",
          "Sue"
        ]
      }
    }
  }
  